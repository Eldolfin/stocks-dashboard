{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["dom", "dom.iterable", "es2022"],
    "moduleDetection": "force",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve"
  },
  "imports": {
    "@sveltejs/kit": "npm:@sveltejs/kit@^2.16.0",
    "@sveltejs/adapter-node": "npm:@sveltejs/adapter-node@^5.2.12",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^6.0.0",
    "svelte": "npm:svelte@^5.0.0",
    "vite": "npm:vite@^7.0.0",
    "chart.js": "npm:chart.js@^4.4.8",
    "chartjs-adapter-luxon": "npm:chartjs-adapter-luxon@^1.3.1",
    "chartjs-adapter-moment": "npm:chartjs-adapter-moment@^1.0.1",
    "chartjs-plugin-zoom": "npm:chartjs-plugin-zoom@^2.2.0",
    "luxon": "npm:luxon@^3.6.1",
    "moment": "npm:moment@^2.30.1",
    "openapi-fetch": "npm:openapi-fetch@^0.14.0",
    "@floating-ui/dom": "npm:@floating-ui/dom@^1.7.3",
    "flowbite": "npm:flowbite@^3.1.2",
    "flowbite-svelte": "npm:flowbite-svelte@^1.0.0",
    "flowbite-svelte-icons": "npm:flowbite-svelte-icons@^2.1.0",
    "svelte-heros-v2": "npm:svelte-heros-v2@^2.0.1",
    "tailwindcss": "npm:tailwindcss@^4.1.11",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.0",
    "@tailwindcss/postcss": "npm:@tailwindcss/postcss@^4.1.11",
    "autoprefixer": "npm:autoprefixer@^10.4.21",
    "prettier": "npm:prettier@^3.4.2",
    "prettier-plugin-svelte": "npm:prettier-plugin-svelte@^3.3.3",
    "prettier-plugin-tailwindcss": "npm:prettier-plugin-tailwindcss@^0.6.11",
    "eslint": "npm:eslint@^9.32.0",
    "eslint-config-prettier": "npm:eslint-config-prettier@^10.0.1",
    "eslint-plugin-svelte": "npm:eslint-plugin-svelte@^3.11.0",
    "svelte-check": "npm:svelte-check@^4.0.0",
    "svelte-eslint-parser": "npm:svelte-eslint-parser@^1.3.1",
    "typescript": "npm:typescript@^5.8.3",
    "typescript-eslint": "npm:typescript-eslint@^8.20.0",
    "globals": "npm:globals@^16.0.0",
    "svelte-language-server": "npm:svelte-language-server@^0.17.17"
  },
  "tasks": {
    "dev": "deno run -A npm:vite dev",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "prepare": "deno run -A npm:svelte-kit sync",
    "check": "deno run -A npm:svelte-kit sync && deno run -A npm:svelte-check --tsconfig ./tsconfig.json",
    "format": "deno run -A npm:prettier --write .",
    "lint": "deno run -A npm:prettier --check . && deno run -A npm:eslint .",
    "generate-api-types": "deno run -A npm:openapi-typescript http://localhost:5000/openapi/openapi.json -o src/generated/api.d.ts",
    "test:ts": "deno check **/*.ts **/*.svelte"
  },
  "nodeModulesDir": true,
  "unstable": ["kv", "temporal"]
}